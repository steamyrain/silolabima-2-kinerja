{
  "name": "kinerja",
  "version": "1.0.0",
  "nodemonConfig": {
    "watch": [
      "src"
    ],
    "ext": "ts",
    "exec": "ts-node ./src/index.ts",
    "env": {
      "NODE_ENV": "development"
    }
  },
  "scripts": {
    "build": "tsc --outDir './dist'",
    "start": "npm run build && NODE_ENV=production node dist/index.js",
    "dev": "nodemon"
  },
  "license": "MIT",
  "dependencies": {
    "cors": "^2.8.5",
    "dotenv": "^10.0.0",
    "dotenv-safe": "^8.2.0",
    "express": "^4.17.1",
    "express-async-wrap": "^1.0.0",
    "mariadb": "^2.5.4",
    "morgan": "^1.10.0",
    "sequelize": "^6.6.5"
  },
  "devDependencies": {
    "@types/cors": "^2.8.12",
    "@types/dotenv-safe": "^8.1.2",
    "@types/express": "^4.17.13",
    "@types/express-async-wrap": "^1.0.0",
    "@types/morgan": "^1.9.3",
    "@types/node": "^16.4.0",
    "@types/validator": "^13.6.3",
    "nodemon": "^2.0.12",
    "ts-node": "^10.1.0",
    "typescript": "^4.3.5"
  }
}
